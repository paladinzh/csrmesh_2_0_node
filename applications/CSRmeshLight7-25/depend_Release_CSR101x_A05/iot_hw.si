  39:iot_hw.c      **** /*----------------------------------------------------------------------------*
  40:iot_hw.c      ****  *  NAME
  41:iot_hw.c      ****  *      IOTLightControlDeviceInit
  42:iot_hw.c      ****  *
  43:iot_hw.c      ****  *  DESCRIPTION
  44:iot_hw.c      ****  *      This function initialises the Red, Green and Blue LED lines.
  45:iot_hw.c      ****  *      Configure the IO lines connected to Switch as inputs.
  46:iot_hw.c      ****  *
  47:iot_hw.c      ****  *  RETURNS
  48:iot_hw.c      ****  *      Nothing.
  49:iot_hw.c      ****  *
  50:iot_hw.c      ****  *---------------------------------------------------------------------------*/
  51:iot_hw.c      **** extern void IOTLightControlDeviceInit(void)
  52:iot_hw.c      **** {
  53:iot_hw.c      **** #ifdef ENABLE_FAST_PWM
  54:iot_hw.c      ****     PioFastPwmConfig(PIO_BIT_MASK(LED_PIO_RED) | \
  55:iot_hw.c      ****                      PIO_BIT_MASK(LED_PIO_GREEN) | \
  56:iot_hw.c      ****                      PIO_BIT_MASK(LED_PIO_BLUE) | \
  57:iot_hw.c      ****                      PIO_BIT_MASK(LED_PIO_W));
  58:iot_hw.c      ****     PioSetModes(1UL << LED_PIO_EN, pio_mode_user);
  59:iot_hw.c      ****     PioSetDir(LED_PIO_EN, PIO_DIRECTION_OUTPUT);  
  60:iot_hw.c      **** 
  61:iot_hw.c      ****     
  62:iot_hw.c      **** 
  63:iot_hw.c      **** #else
  64:iot_hw.c      ****     /* Configure the LED_PIO_RED PIO as output PIO */
  65:iot_hw.c      ****     PioSetDir(LED_PIO_RED, PIO_DIRECTION_OUTPUT);
  66:iot_hw.c      **** 
  67:iot_hw.c      ****     /* Configure the LED_PIO_GREEN PIO as output PIO */
  68:iot_hw.c      ****     PioSetDir(LED_PIO_GREEN, PIO_DIRECTION_OUTPUT);
  69:iot_hw.c      **** 
  70:iot_hw.c      ****     /* Configure the LED_PIO_BLUE PIO as output PIO */
  71:iot_hw.c      ****     PioSetDir(LED_PIO_BLUE, PIO_DIRECTION_OUTPUT);
  72:iot_hw.c      **** #endif /* ENABLE_FAST_PWM */
  73:iot_hw.c      **** }
  74:iot_hw.c      **** 
  75:iot_hw.c      **** /*----------------------------------------------------------------------------*
  76:iot_hw.c      ****  *  NAME
  77:iot_hw.c      ****  *      IOTLightControlDevicePower
  78:iot_hw.c      ****  *
  79:iot_hw.c      ****  *  DESCRIPTION
  80:iot_hw.c      ****  *      This function sets power state of LED.
  81:iot_hw.c      ****  *
  82:iot_hw.c      ****  *  RETURNS
  83:iot_hw.c      ****  *      Nothing.
  84:iot_hw.c      ****  *
  85:iot_hw.c      ****  *---------------------------------------------------------------------------*/
  86:iot_hw.c      **** extern void IOTLightControlDevicePower(bool power_on)
  87:iot_hw.c      **** {
  88:iot_hw.c      **** #ifdef ENABLE_FAST_PWM
  89:iot_hw.c      ****     PioFastPwmEnable(power_on);
  90:iot_hw.c      **** #else
  91:iot_hw.c      ****     if (power_on == TRUE)
  92:iot_hw.c      ****     {
  93:iot_hw.c      ****         /* Configure the LED's */
  94:iot_hw.c      **** 
  95:iot_hw.c      ****         /* Connect PWM0 output to LED_PIO_RED */
  96:iot_hw.c      ****         PioSetMode(LED_PIO_RED, pio_mode_pwm0);
  97:iot_hw.c      **** 
  98:iot_hw.c      ****         /* Connect PWM1 output to LED_PIO_GREEN */
  99:iot_hw.c      ****         PioSetMode(LED_PIO_GREEN, pio_mode_pwm1);
 100:iot_hw.c      **** 
 101:iot_hw.c      ****          /* Connect PWM2 output to LED_PIO_BLUE */
 102:iot_hw.c      ****         PioSetMode(LED_PIO_BLUE, pio_mode_pwm2);
 103:iot_hw.c      **** 
 104:iot_hw.c      ****         /* Enable the PIO's */
 105:iot_hw.c      ****         PioEnablePWM(LED_PWM_RED, TRUE);
 106:iot_hw.c      ****         PioEnablePWM(LED_PWM_GREEN, TRUE);
 107:iot_hw.c      ****         PioEnablePWM(LED_PWM_BLUE, TRUE);
 108:iot_hw.c      ****     }
 109:iot_hw.c      ****     else
 110:iot_hw.c      ****     {
 111:iot_hw.c      ****          /* When power off is selected, disable all PWMs and
 112:iot_hw.c      ****           * set all PIOs to HIGH, as IOT board uses common anode LED.
 113:iot_hw.c      ****           */
 114:iot_hw.c      ****          PioSetMode(LED_PIO_RED, pio_mode_user);
 115:iot_hw.c      ****          PioSetMode(LED_PIO_GREEN, pio_mode_user);
 116:iot_hw.c      ****          PioSetMode(LED_PIO_BLUE, pio_mode_user);
 117:iot_hw.c      **** 
 118:iot_hw.c      ****          PioSet(LED_PIO_RED, 1);
 119:iot_hw.c      ****          PioSet(LED_PIO_GREEN, 1);
 120:iot_hw.c      ****          PioSet(LED_PIO_BLUE, 1);
 121:iot_hw.c      ****     }
 122:iot_hw.c      **** #endif
 123:iot_hw.c      **** }
 124:iot_hw.c      **** 
 125:iot_hw.c      **** /*----------------------------------------------------------------------------*
 126:iot_hw.c      ****  *  NAME
 127:iot_hw.c      ****  *      IOTLightControlDeviceSetLevel
 128:iot_hw.c      ****  *
 129:iot_hw.c      ****  *  DESCRIPTION
 130:iot_hw.c      ****  *      This function sets the brightness level.
 131:iot_hw.c      ****  *      Convert Level to equal values of RGB.
 132:iot_hw.c      ****  *      Note that linear translation has been assumed for now.
 133:iot_hw.c      ****  *
 134:iot_hw.c      ****  *  RETURNS
 135:iot_hw.c      ****  *      Nothing.
 136:iot_hw.c      ****  *
 137:iot_hw.c      ****  *---------------------------------------------------------------------------*/
 138:iot_hw.c      **** extern void IOTLightControlDeviceSetLevel(uint8 level)
 139:iot_hw.c      **** {
 140:iot_hw.c      ****     /* Maps level to equal values of Red, Green and Blue */
 141:iot_hw.c      ****     IOTLightControlDeviceSetColor(level, level, level);
 142:iot_hw.c      **** }
 143:iot_hw.c      **** 
 144:iot_hw.c      **** /*----------------------------------------------------------------------------*
 145:iot_hw.c      ****  *  NAME
 146:iot_hw.c      ****  *      IOTLightControlDeviceSetColor
 147:iot_hw.c      ****  *
 148:iot_hw.c      ****  *  DESCRIPTION
 149:iot_hw.c      ****  *      This function sets the colour as passed in argument values.
 150:iot_hw.c      ****  *
 151:iot_hw.c      ****  *  RETURNS
 152:iot_hw.c      ****  *      Nothing.
 153:iot_hw.c      ****  *
 154:iot_hw.c      ****  *---------------------------------------------------------------------------*/
 155:iot_hw.c      **** extern void IOTLightControlDeviceSetColor(uint8 red, uint8 green, uint8 blue)
 156:iot_hw.c      **** {
 157:iot_hw.c      **** #ifdef ENABLE_FAST_PWM
 158:iot_hw.c      ****    // PioFastPwmSetWidth(LED_PIO_RED, red, 0xFF - red, TRUE);
 159:iot_hw.c      ****    // PioFastPwmSetWidth(LED_PIO_GREEN, green, 0xFF - green, TRUE);
 160:iot_hw.c      ****    // PioFastPwmSetWidth(LED_PIO_BLUE, blue, 0xFF - blue, TRUE);
 161:iot_hw.c      **** 	uint8 white = 0;
 162:iot_hw.c      **** 
 163:iot_hw.c      **** 	if(red<=green&&red<=blue)white = red;
 164:iot_hw.c      **** 	if(green<=red&&green<=blue)white = green;
 165:iot_hw.c      **** 	if(blue<=red&&blue<=green)white = blue;
 166:iot_hw.c      **** 
 167:iot_hw.c      ****     PioFastPwmSetWidth(LED_PIO_RED, 0xff-red,  red, TRUE);
 168:iot_hw.c      ****     PioFastPwmSetWidth(LED_PIO_GREEN, 0xff-green, green, TRUE);
 169:iot_hw.c      ****     PioFastPwmSetWidth(LED_PIO_BLUE, 0xff-blue, blue, TRUE);   
 170:iot_hw.c      **** 	PioFastPwmSetWidth(LED_PIO_W, 0xff-white, white, TRUE);
 171:iot_hw.c      ****     PioFastPwmSetPeriods(1, 0);
 172:iot_hw.c      ****     PioFastPwmEnable(TRUE);
 173:iot_hw.c      **** #else
 174:iot_hw.c      ****     /* When level is Lowest (0-3) simply disable PWM to avoid flicker */
 175:iot_hw.c      ****     if ((red >> QUANTIZATION_ERROR) == 0)
 176:iot_hw.c      ****     {
 177:iot_hw.c      ****          PioSetMode(LED_PIO_RED, pio_mode_user);
 178:iot_hw.c      ****          PioSet(LED_PIO_RED, 1);
 179:iot_hw.c      ****     }
 180:iot_hw.c      ****     else
 181:iot_hw.c      ****     {
 182:iot_hw.c      ****         PioSetMode(LED_PIO_RED, pio_mode_pwm0);
 183:iot_hw.c      ****     }
 184:iot_hw.c      **** 
 185:iot_hw.c      ****     if ((green >> QUANTIZATION_ERROR) == 0)
 186:iot_hw.c      ****     {
 187:iot_hw.c      ****          PioSetMode(LED_PIO_GREEN, pio_mode_user);
 188:iot_hw.c      ****          PioSet(LED_PIO_GREEN, 1);
 189:iot_hw.c      ****     }
 190:iot_hw.c      ****     else
 191:iot_hw.c      ****     {
 192:iot_hw.c      ****         PioSetMode(LED_PIO_GREEN, pio_mode_pwm1);
 193:iot_hw.c      ****     }
 194:iot_hw.c      **** 
 195:iot_hw.c      ****     if ((blue >> QUANTIZATION_ERROR) == 0)
 196:iot_hw.c      ****     {
 197:iot_hw.c      ****          PioSetMode(LED_PIO_BLUE, pio_mode_user);
 198:iot_hw.c      ****          PioSet(LED_PIO_BLUE, 1);
 199:iot_hw.c      ****     }
 200:iot_hw.c      ****     else
 201:iot_hw.c      ****     {
 202:iot_hw.c      ****         PioSetMode(LED_PIO_BLUE, pio_mode_pwm2);
 203:iot_hw.c      ****     }
 204:iot_hw.c      **** 
 205:iot_hw.c      ****     /* Invert values as its a pull down */
 206:iot_hw.c      ****     red   = COLOR_MAX_VALUE - (red >> QUANTIZATION_ERROR);
 207:iot_hw.c      ****     green = COLOR_MAX_VALUE - (green >> QUANTIZATION_ERROR);
 208:iot_hw.c      ****     blue  = COLOR_MAX_VALUE - (blue >> QUANTIZATION_ERROR);
 209:iot_hw.c      **** 
 210:iot_hw.c      ****     PioConfigPWM(LED_PWM_RED, pio_pwm_mode_push_pull,
 211:iot_hw.c      ****                  red, (COLOR_MAX_VALUE - red), 1U,
 212:iot_hw.c      ****                  red, (COLOR_MAX_VALUE - red), 1U, 0U);
 213:iot_hw.c      ****     PioConfigPWM(LED_PWM_GREEN, pio_pwm_mode_push_pull,
 214:iot_hw.c      ****                  green, (COLOR_MAX_VALUE - green), 1U,
 215:iot_hw.c      ****                  green, (COLOR_MAX_VALUE - green), 1U, 0U);
 216:iot_hw.c      ****     PioConfigPWM(LED_PWM_BLUE, pio_pwm_mode_push_pull,
 217:iot_hw.c      ****                  blue, (COLOR_MAX_VALUE - blue), 1U,
 218:iot_hw.c      ****                  blue, (COLOR_MAX_VALUE - blue), 1U, 0U);
 219:iot_hw.c      **** #endif /* ENABLE_FAST_PWM */
 220:iot_hw.c      **** }
 221:iot_hw.c      **** 
 222:iot_hw.c      **** /*----------------------------------------------------------------------------*
 223:iot_hw.c      ****  *  NAME
 224:iot_hw.c      ****  *      IOTLightControlDeviceBlink
 225:iot_hw.c      ****  *
 226:iot_hw.c      ****  *  DESCRIPTION
 227:iot_hw.c      ****  *      This function sets colour and blink time for LEDs.
 228:iot_hw.c      ****  *
 229:iot_hw.c      ****  *  RETURNS
 230:iot_hw.c      ****  *      Nothing.
 231:iot_hw.c      ****  *
 232:iot_hw.c      ****  *---------------------------------------------------------------------------*/
 233:iot_hw.c      **** extern void IOTLightControlDeviceBlink(uint8 red, uint8 green, uint8 blue,
 234:iot_hw.c      ****                                        uint8 on_time, uint8 off_time)
 235:iot_hw.c      **** {
 236:iot_hw.c      **** #ifdef ENABLE_FAST_PWM    
 237:iot_hw.c      ****     PioSet(LED_PIO_EN,1);    
 238:iot_hw.c      ****     PioFastPwmSetWidth(LED_PIO_RED, 255, red, TRUE);
 239:iot_hw.c      ****     PioFastPwmSetWidth(LED_PIO_GREEN, 255, green, TRUE);
 240:iot_hw.c      ****     PioFastPwmSetWidth(LED_PIO_BLUE, 255, blue, TRUE);
 241:iot_hw.c      ****     PioFastPwmSetPeriods((on_time << 4), (off_time << 4));
 242:iot_hw.c      ****     PioFastPwmEnable(TRUE);  
 243:iot_hw.c      **** 
 244:iot_hw.c      **** #else
 245:iot_hw.c      ****     IOTLightControlDevicePower(TRUE);
 246:iot_hw.c      **** 
 247:iot_hw.c      ****     IOTLightControlDeviceSetColor(red, green, blue);
 248:iot_hw.c      **** 
 249:iot_hw.c      ****     /* Invert the On and Off times as LEDs on
 250:iot_hw.c      ****      * IOT board are Common-Anode type
 251:iot_hw.c      ****      */
 252:iot_hw.c      ****     PioConfigPWM(LED_PWM_RED, pio_pwm_mode_push_pull,
 253:iot_hw.c      ****                  red, 0, off_time, 0, red, on_time, 0U);
 254:iot_hw.c      **** 
 255:iot_hw.c      ****     PioConfigPWM(LED_PWM_GREEN, pio_pwm_mode_push_pull,
 256:iot_hw.c      ****                  green, 0, off_time, 0, green, on_time, 0U);
 257:iot_hw.c      **** 
 258:iot_hw.c      ****     PioConfigPWM(LED_PWM_BLUE, pio_pwm_mode_push_pull,
 259:iot_hw.c      ****                  blue, 0, off_time, 0, blue, on_time, 0U);
 260:iot_hw.c      **** #endif
 261:iot_hw.c      **** }
