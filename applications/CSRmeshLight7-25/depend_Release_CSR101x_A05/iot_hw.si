  25:iot_hw.c      **** /* Colour depth in bits, as passed by application. */
  26:iot_hw.c      **** #define LIGHT_INPUT_COLOR_DEPTH  (8)
  27:iot_hw.c      **** /* Colour depth in bits, mapped to actual hardware. */
  28:iot_hw.c      **** #define LIGHT_MAPPED_COLOR_DEPTH (6)
  29:iot_hw.c      **** /* Colour depth lost due to re-quantization of levels. */
  30:iot_hw.c      **** #define QUANTIZATION_ERROR       (LIGHT_INPUT_COLOR_DEPTH -\
  31:iot_hw.c      ****                                   LIGHT_MAPPED_COLOR_DEPTH)
  32:iot_hw.c      **** /* Maximum colour level supported by mapped colour depth bits. */
  33:iot_hw.c      **** #define COLOR_MAX_VALUE          ((0x1 << LIGHT_MAPPED_COLOR_DEPTH) - 1)
  34:iot_hw.c      **** 
